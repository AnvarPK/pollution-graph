onDataByDate(selectedDate));\n    }, [selectedDate]);\n\n    //date picker date chage handler\n    //to load pollution data by selected date\n    const handleDateChange = (event) => {\n        let selected = event.target.value;\n        let dateObj = {\n            date_from: selected,\n            date_to: moment(selected).add(1, \"days\").format('YYYY-MM-DD')\n        }\n        setSelectedDate(dateObj);\n    };\n\n    return <form className={classes.container} noValidate>\n        <TextField\n            id=\"date\"\n            label=\"Select Date\"\n            type=\"date\"\n            defaultValue={selectedDate.date_from}\n            className={classes.textField}\n            InputLabelProps={{\n                shrink: true,\n            }}\n            onChange={handleDateChange}\n        />\n    </form>\n\n}\n\nexport default connect()(DatePicker)","/media/ab/OS/ANVAR/personal/Interviews/sinergia/pollution-graph/src/component/location-list.js",[],"/media/ab/OS/ANVAR/personal/Interviews/sinergia/pollution-graph/src/component/graph-container.js",[],"/media/ab/OS/ANVAR/personal/Interviews/sinergia/pollution-graph/src/component/graph.js",[],{"ruleId":"59","replacedBy":"60"},{"ruleId":"61","replacedBy":"62"},{"ruleId":"63","severity":1,"message":"64","line":8,"column":22,"nodeType":"65","messageId":"66","endLine":8,"endColumn":25},{"ruleId":"67","severity":1,"message":"68","line":5,"column":1,"nodeType":"69","endLine":16,"endColumn":2},{"ruleId":"70","severity":1,"message":"71","line":9,"column":13,"nodeType":"72","messageId":"73","endLine":9,"endColumn":19},{"ruleId":"70","severity":1,"message":"71","line":13,"column":13,"nodeType":"72","messageId":"73","endLine":13,"endColumn":19},{"ruleId":"74","severity":1,"message":"75","line":5,"column":32,"nodeType":"76","messageId":"77","endLine":5,"endColumn":34},{"ruleId":"78","severity":1,"message":"79","line":35,"column":8,"nodeType":"80","endLine":35,"endColumn":22,"suggestions":"81"},"no-native-reassign",["82"],"